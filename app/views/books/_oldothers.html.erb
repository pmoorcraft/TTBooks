<% @oldother_feed.each do |item| %>
	<% if item.is_a? Book %>
	<div class="container" id="content-row">
	  
		  <div class="toggler" onclick="toggleText('<%= item.imageurl %><%= item.id %>');" >
		  
			  <!-- Content before clicking goes here -->
			 
			  <div class="thumbnail1" style="float: left; margin-left: 20px; margin-right: 14px;">
				<img  src="<%= item.imageurl %>" width="75" heigth="110" style="margin-left: 10px; margin-right: 20px; margin-bottom: 15px;">
			  </div>
			  
			   <div style="float:right; margin-right:20px; margin-bottom: -2px;">
			  	<%= item.release %>
			  </div>
			  
			  <div>
			  <h2><%= item.title%></h2><i class="fa fa-book fa-2x"></i>
			  <p><%= item.author%></p>  			  
			  </div>
			  
			  <div style="float: left;">
			  <button type="button" class="btn btn-warning"><span class="glyphicon glyphicon-tags" aria-hidden="true" style="padding-right: 7px;"></span><%= item.genre %></button>
			  </div>
			  
			  <!-- Content before clicking ends here -->
			  <div id="<%= item.imageurl %><%= item.id %>" style="display:none;" style="background-color: rgb(170, 170, 170);">
			  <!-- Content after clicking goes here -->  
				<div class= "container" style="margin-top: 100px; ">
					<div class= "col-md-6" style="margin-right: 10px; margin-left: 10px; margin-bottom: 10px; width: auto; float: left;">
				     <a href="<%= item.amazon %>"><img src="<%= item.imageurl %>" style="display: block; border-radius: 5px; margin-left:auto; margin-right: auto; margin-bottom: auto;"></a> 
			  			</div>
				     <div class="col-md-6">
				     	<div class="container" style="float:left; width:100%;">
				     	<p><%= item.header %></p>
				     	<button class="btn btn-small" disabled style="margin-left: 1px; margin-bottom:auto; float: right;"><%= pluralize(item.votes.count, "vote") %></button>
				     	<%= button_to '+1', upvotebook_book_path(item), method: :post, class: 'btn btn-small', style: "margin-left: 1px; float: right;" %>
			 	    	<% if item.free_book == "true" %>
		     				<button class="btn btn-small" style="margin-left: 5px; margin-right: 5px; margin-top: -1px; float:right;"><a href="<%= item.amazon %>" style="color: black;">Free Book- Read Now</a></button>	
		     			<% end %>
			 	    	
			 	    	<div class="container">
			 	    	<a href="http://news.ycombinator.com/submit" class="hn-share-button" style="margin-top: 10px;">Vote on HN</a>
					 	
						
						
						<a href="https://twitter.com/share" class="twitter-share-button" data-via="TechTalksBooks" data-related="TechTalksBooks" data-dnt="true" data-align="right" style="margin-top: 10px;">Tweet</a>
						<script>!function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0],p=/^http:/.test(d.location)?'http':'https';if(!d.getElementById(id)){js=d.createElement(s);js.id=id;js.src=p+'://platform.twitter.com/widgets.js';fjs.parentNode.insertBefore(js,fjs);}}(document, 'script', 'twitter-wjs');</script>
						
			 	    	
			 	    	
		     			</div>
		     			
				     <br style="clear:both;"/>
				     </div>
					</div>
					</div>
			</div>
			   
			   <!-- Content after clicking ends here -->
		   
		</div>
	</div>
	
	<script>
		function toggleText(id) {
	    var theText = document.getElementById(id);
	 	 (theText.style.display=='block') ? theText.style.display='none' : theText.style.display='block' ;  
		}
	</script>

		
    <% else item.is_a? Video %>
		<div class="container" id="content-row">
	  
		  <div class="toggler" onclick="toggleText('<%= item.image_url %><%= item.id %>');" style="cursor:pointer; position: relative">
		  
			  <!-- Content before clicking goes here -->
			 
			  <div class="thumbnail1" style="float: left; style="margin-left: 20px; margin-right: 20px;">
	
			  	<img  src="http://i1.ytimg.com/vi/<%= item.image_url %>/default.jpg" width="110" heigth="63" style="margin-left: 10px; margin-right: 20px; margin-bottom: 15px;">
			  </div>
			  
			  <div style="float:right;">
			  	<%= item.uploaded %>
			  </div>
			  
			  <div>
			  <h2><%= item.title%></h2><i class="fa fa-play-circle-o fa-2x"></i>
			  <p><%= item.speakers%></p>			  
			  </div>
			  
			  <div style="float: left;">
			  <button type="button" class="btn btn-warning"><span class="glyphicon glyphicon-tags" aria-hidden="true" style="padding-right: 7px;"></span><%= item.genre %></button>
			  </div>
			  
			  <!-- Content before clicking ends here -->
			  <div id="<%= item.image_url %><%= item.id %>" style="display:none;">
			  <!-- Content after clicking goes here -->  
				<div class= "container" style="margin-top: 100px; ">
					<div class= "col-md-6" style="  margin-right: auto; margin-left: auto; margin-bottom: 10px; float: left;">
				     <iframe width="500" height="315" style="border-radius: 5px; margin-left:auto; margin-right: auto; margin-bottom: auto; "src="<%= item.url %>" frameborder="0" "allowfullscreen"></iframe> 
				  					    </div>
				     <div class="col-md-6">
				     	
				     	<div class="container" style="float:left; width:100%;">
				     	<p><%= item.description %></p>
				     	<button class="btn btn-small" disabled style="margin-left: 1px; margin-bottom:auto; float: right;"><%= pluralize(item.votes.count, "vote") %></button>
				     	<%= button_to '+1', upvotevideo_video_path(item), method: :post, class: 'btn btn-small', style: "margin-left: 1px; float: right;" %>
			 	    	
			 	    	<div class="container">
			 	    	<a href="http://news.ycombinator.com/submit" class="hn-share-button">Vote on HN</a>
					 	<script type="text/javascript">
			 				reddit_bgcolor = "FF3";
			  				reddit_bordercolor = "00F";
						</script>
						
						
						<script type="text/javascript" src="//www.redditstatic.com/button/button1.js" style="margin-right: 10px;"></script>
				 	  	<a href="https://twitter.com/share" class="twitter-share-button" data-via="TechTalksBooks" data-related="TechTalksBooks" data-dnt="true" data-align="right" >Tweet</a>
						<script>!function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0],p=/^http:/.test(d.location)?'http':'https';if(!d.getElementById(id)){js=d.createElement(s);js.id=id;js.src=p+'://platform.twitter.com/widgets.js';fjs.parentNode.insertBefore(js,fjs);}}(document, 'script', 'twitter-wjs');</script>
						</div>
			 	    	
				     
				     <br style="clear:both;"/>
				     </div>
					</div>
					</div>
			</div>
			   
			   <!-- Content after clicking ends here -->
		   
		</div>
	</div>
		
		<script>
		function toggleText(id) {
	    var theText = document.getElementById(id);
	 	 (theText.style.display=='block') ? theText.style.display='none' : theText.style.display='block' ;  
		}
		</script>	
	<% end %>
	
<% end %>