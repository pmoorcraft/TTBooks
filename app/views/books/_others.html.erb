<% @other_feed.each do |item| %>
	<% if item.is_a? Book %>
		<div class="container" id="content-row">
	  
		  <div class="toggler" onclick="toggleText('<%= item.imageurl %><%= item.id %>');" >
		  
			  <!-- Content before clicking goes here -->
			 
			  <div class="thumbnail1" style="float: left; margin-left: 20px; margin-right: 20px;">
				<img  src="<%= item.imageurl %>" width="75" heigth="110" style="margin-left: 10px; margin-right: 20px; margin-bottom: 15px;">
			  </div>
			  
			   <div style="float:right; margin-right:20px; margin-bottom: -2px;">
			  	<%= item.release %>
			  </div>
			  
			  <div>
			  <h2><%= item.title%></h2><i class="fa fa-book fa-2x"></i>
			  <p><%= item.author%></p>  		  
			  </div>
			  
			  <div style="float: left;">
			  <button type="button" class="btn btn-warning"><span class="glyphicon glyphicon-tags" aria-hidden="true" style="padding-right: 7px;"></span><%= item.genre %></button>
			  </div>
			  
			  <!-- Content before clicking ends here -->
			  <div id="<%= item.imageurl %><%= item.id %>" style="display:none;" style="background-color: rgb(170, 170, 170);">
			  <!-- Content after clicking goes here -->  
				<div class= "container" style="margin-top: 100px; ">
					<div class= "col-md-6" style="  margin-right: 10px; margin-left: 10px; margin-bottom: 10px; width: auto; float: left;">
				     <a href="<%= item.amazon %>"><img src="<%= item.imageurl %>" style="display: block; border-radius: 5px; margin-left:auto; margin-right: auto; margin-bottom: auto;"></a> 
			  			</div>
				     <div class="col-md-8">
				     	<div class="container" style="float:left; width:100%;">
				     	<p><%= item.header %></p>
				     	<button class="btn btn-warning btn-small" style="margin-left: 1px; margin-bottom:auto; float: right; cursor: default; margin-left: 5px;"><%= pluralize(item.votes.count, "vote") %></button>
				     	<%= button_to '+1', upvotebook_book_path(item), method: :post, class: 'btn btn-warning btn-small', style: "margin-left: 1px; float: right;" %>
			 	    	<% if item.free_book == "true" %>
		     				<button class="btn btn-warning btn-small" style="margin-left: 5px; margin-right: 5px; margin-top: -1px; float:right;"><a style= "color: white;" href="<%= item.amazon %>">Free Book- Read Now</a></button>	
		     			<% end %>
			 	    	
			 	    	<div class="container">
			 	    	<a class="resp-sharing-button__link" href="https://facebook.com/sharer/sharer.php?u=http%3A%2F%2Fwww.ttbooks.io/books/<%= item.id %>" target="_blank"><div class="resp-sharing-button resp-sharing-button--facebook resp-sharing-button--small">
						    <svg class="resp-sharing-button__icon" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="1em" height="1em" viewBox="0 0 32 32">
						<path fill="#FFF" d="M26.667 0h-21.334c-2.945 0-5.333 2.388-5.333 5.334v21.332c0 2.946 2.387 5.334 5.333 5.334h10.667v-14h-4v-4h4v-3c0-2.761 2.239-5 5-5h5v4h-5c-0.552 0-1 0.448-1 1v3h5.5l-1 4h-4.5v14h6.667c2.945 0 5.333-2.388 5.333-5.334v-21.332c0-2.946-2.387-5.334-5.333-5.334z"></path>
						</svg>
						</div>
						</a>
						<a class="resp-sharing-button__link" href="https://twitter.com/intent/tweet/?text=<%= item.title %>&url=http%3A%2F%2Fwww.ttbooks.io/books/<%= item.id %>" target="_blank"><div class="resp-sharing-button resp-sharing-button--twitter resp-sharing-button--small">
						    <svg class="resp-sharing-button__icon" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="1em" height="1em" viewBox="0 0 32 32">
						<path fill="#FFF" d="M26.667 0h-21.333c-2.934 0-5.334 2.4-5.334 5.334v21.332c0 2.936 2.4 5.334 5.334 5.334h21.333c2.934 0 5.333-2.398 5.333-5.334v-21.332c0-2.934-2.399-5.334-5.333-5.334zM23.952 11.921c0.008 0.176 0.012 0.353 0.012 0.531 0 5.422-4.127 11.675-11.675 11.675-2.317 0-4.474-0.679-6.29-1.844 0.321 0.038 0.648 0.058 0.979 0.058 1.922 0 3.692-0.656 5.096-1.757-1.796-0.033-3.311-1.219-3.833-2.849 0.251 0.048 0.508 0.074 0.772 0.074 0.374 0 0.737-0.050 1.081-0.144-1.877-0.377-3.291-2.035-3.291-4.023 0-0.017 0-0.034 0-0.052 0.553 0.307 1.186 0.492 1.858 0.513-1.101-0.736-1.825-1.992-1.825-3.415 0-0.752 0.202-1.457 0.556-2.063 2.024 2.482 5.047 4.116 8.457 4.287-0.070-0.3-0.106-0.614-0.106-0.935 0-2.266 1.837-4.103 4.103-4.103 1.18 0 2.247 0.498 2.995 1.296 0.935-0.184 1.813-0.525 2.606-0.996-0.306 0.958-0.957 1.762-1.804 2.27 0.83-0.099 1.621-0.32 2.357-0.646-0.55 0.823-1.245 1.545-2.047 2.124z"></path>
						</svg>
						</div>
						</a>	 	    	
			 	    	
		     			</div>
		     			
				     <br style="clear:both;"/>
				     </div>
					</div>
					</div>
			</div>
			   
			   <!-- Content after clicking ends here -->
		   
		</div>
	</div>
	
	<script>
		function toggleText(id) {
	    var theText = document.getElementById(id);
	 	 (theText.style.display=='block') ? theText.style.display='none' : theText.style.display='block' ;  
		}
	</script>
		
    <% else item.is_a? Video %>
		<div class="container" id="content-row">
	  
		  <div class="toggler" onclick="toggleText('<%= item.image_url %><%= item.id %>');" style="cursor:pointer; position: relative">
		  
			  <!-- Content before clicking goes here -->
			 
			  <div class="thumbnail1" style="float: left; style="margin-left: 20px; margin-right: 14px;">
	
			  	<img  src="http://i1.ytimg.com/vi/<%= item.image_url %>/default.jpg" width="110" heigth="63" style="margin-left: 10px; margin-right: 20px; margin-bottom: 15px;">
			  </div>
			  
			  <div style="float:right;">
			  	<%= item.uploaded %>
			  </div>
			  
			  <div>
			  <h2><%= item.title%></h2><i class="fa fa-play-circle-o fa-2x"></i>
			  <p><%= item.speakers%></p>			  
			  </div>
			  
			  <div style="float: left;">
			  <button type="button" class="btn btn-warning"><span class="glyphicon glyphicon-tags" aria-hidden="true" style="padding-right: 7px;"></span><%= item.genre %></button>
			  </div>
			  
			  <!-- Content before clicking ends here -->
			  <div id="<%= item.image_url %><%= item.id %>" style="display:none;">
			  <!-- Content after clicking goes here -->  
				<div class= "container" style="margin-top: 100px; ">
					<div class= "col-md-6" style="  margin-right: auto; margin-left: auto; margin-bottom: 10px; float: left;">
				     <iframe width="500" height="315" style="border-radius: 5px; margin-left:auto; margin-right: auto; margin-bottom: auto; "src="<%= item.url %>" frameborder="0" "allowfullscreen"></iframe> 
				  					    </div>
				     <div class="col-md-6">
				     	<div class="container" style="float:left; width:100%;">
				     	<p><%= item.description %></p>
				     	<button class="btn btn-warning btn-small" style="margin-left: 1px; margin-bottom:auto; float: right; cursor: default; margin-left: 5px;"><%= pluralize(item.votes.count, "vote") %></button>
				     	<%= button_to '+1', upvotevideo_video_path(item), method: :post, class: 'btn btn-warning btn-small', style: "margin-left: 1px; float: right;" %>
			 	    	
			 	    	<div class="container">
			 	    	<a class="resp-sharing-button__link" href="https://facebook.com/sharer/sharer.php?u=http%3A%2F%2Fwww.ttbooks.io/books/<%= item.id %>" target="_blank"><div class="resp-sharing-button resp-sharing-button--facebook resp-sharing-button--small">
						    <svg class="resp-sharing-button__icon" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="1em" height="1em" viewBox="0 0 32 32">
