<% @operatorvideos.each do |video| %>

	  <div class="container" id="content-row" style=" border-radius: 5px; background-color: white; margin-bottom: 20px;">
	  
	  <div class="toggler" onclick="toggleText('toToggle');" style="cursor:pointer">
	  <!-- Content before clicking goes here -->
	  <div class="thumbnail1" style="float: left; style="margin-left: 20px; margin-right: 20px;">
	  <img  src="http://i1.ytimg.com/vi/<%= video.image_url %>/hqdefault.jpg" width="150" heigth="83" style="margin-left: 20px; margin-right: 20px; margin-top: 20px; margin-bottom: 20px;">
	  </div>
	  
	  <div style="float:right;">
	  <%= video.uploaded %>
	  </div>
	  <div>
	  <h2><%= video.title%></h2>
	  <p><%= video.description %></p>
	  
	  </div>
	  
	  <!-- Content before clicking ends here -->
	  <div id="toToggle" style="display:none;">
	  <!-- Content after clicking goes here -->  
	    <table>
	    <td>
	     <iframe width="420" height="315" src="<%= video.url %>" frameborder="0" allowfullscreen"></iframe> 
	    
	    </td>
	    <td>
	    
	   
	    <tr><%= button_to '+1', upvotevideo_video_path(video), method: :post, class: 'btn btn-small' %>
 	    <button class="btn btn-small" disabled style="margin-left: 1px;"><%= pluralize(video.votes.count, "vote") %></button>
	    </tr>
	    </td>
	    </table>
	   </div>
	   
	   <!-- Content after clicking ends here -->
	  </div>
	  </div>
	
	<script>
	function toggleText(id) {
    var theText = document.getElementById(id);
 	 (theText.style.display=='block') ? theText.style.display='none' : theText.style.display='block' ;  
	}
	</script>
	<% end %>

