<%= form_for @video, url: {action: "create"}, html: {class: "nifty_form"} do |f| %>
  <ul>
  Title
  <%= f.text_field :title %>
  </ul>
  <ul>
  Embed URL
  <%= f.text_field :url %>
  </ul>
  <ul>
  Upload date
  <%= f.date_field :uploaded %>
  </ul>
  <ul>
  Description
  <%= f.text_area :description, size: "40x5" %>
  </ul>
  <ul>
  Speakers
  <%= f.text_field :speakers %>
  </ul>
  <ul>
  Tag
  <%= f.text_field :tag %>
  </ul>
  <ul>
  Genre (i.e- tag button)
  <%= f.text_field :genre %>
  </ul>
  <ul>
  <%= f.submit "Create" %>
  </ul>
<% end %>