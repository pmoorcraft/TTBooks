Edit
<%= form_for @video do |f| %>
  <ul>
  Title
  <%= f.text_field :title %>
  </ul>
  <ul>
  Embed URL
  <%= f.text_field :url %>
  </ul>
  <ul>
  Thumbnail URL
  <%= f.text_field :image_url %>
  </ul>
  <ul>
  Upload date
  <%= f.date_field :uploaded %>
  </ul>
  <ul>
  Description
  <%= f.text_area :description, size: "40x5" %>
  </ul>
  <ul>
  Speakers
  <%= f.text_field :speakers %>
  </ul>
  <ul>
  Tag
  <%= f.text_field :tag %>
  </ul>
  <ul>
  Genre (i.e- tag button)
  <%= f.text_field :genre %>
  </ul>
  <ul>
  related1
  <%= f.text_field :related1 %>
  </ul><ul>
  related1name
  <%= f.text_field :related1name %>
  </ul><ul>
  related2
  <%= f.text_field :related2 %>
  </ul><ul>
  related2name
  <%= f.text_field :related2name %>
  </ul><ul>
  related3
  <%= f.text_field :related3 %>
  </ul><ul>
  related3name
  <%= f.text_field :related3name %>
  </ul>
  <ul>
  <%= f.submit "Create" %>
  </ul>
<% end %>